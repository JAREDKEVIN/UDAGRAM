    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.2",
    "aws-sdk": "^2.492.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "email-validator": "^2.0.4",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.10.0",
    "sequelize-typescript": "^0.6.11"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/express": "^4.17.9",
    "@types/node": "^11.15.42",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^10.11.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.3",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JAREDKEVIN/UDAGRAM.git"
